<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xhu.dao.VersionDao">
        <select id="getVersion" resultType="com.xhu.entity.Version">
            SELECT * FROM version
            ORDER BY id
            limit 0,1
        </select>

        <insert id="add" parameterType="com.xhu.entity.Version">
                INSERT INTO version(number,developer,uptime)
                VALUES (#{number},#{developer},#{uptime})
        </insert>
</mapper>